         <ref name="att.global.attributes"/>
         <ref name="att.spanning.attributes"/>
         <optional>
            <attribute name="indexName">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">donne un nom pour préciser à quel index (parmi plusieurs) appartient l'entrée d'index.</a:documentation>
